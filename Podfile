# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'MoneyCheck (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MoneyCheck (iOS)

	pod 'Firebase/Auth'
	pod 'Firebase/Firestore'

end

target 'MoneyCheck (macOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MoneyCheck (macOS)

	pod 'Firebase/Auth'
	pod 'Firebase/Firestore'

end
